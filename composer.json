{
  "name": "chasegiunta/craft-vue",
  "description": "Craft CMS + Vue.js Boilerplate",
  "keywords": [
    "craft", "cms", "craftcms", "project", "vue", "vue.js", "vue js"
  ],
  "license": "Unlicense",
  "homepage": "https://craftcms.com/",
  "type": "project",
  "require": {
    "craftcms/cms": "3.2.3",
    "vlucas/phpdotenv": "^2.4.0",
    "nystudio107/craft-twigpack": "1.1.7",
    "xpertbot/craft-wheelform": "2.2.1",
    "craftcms/element-api": "2.5.4",
    "craftcms/redactor": "2.3.3.2",
    "verbb/super-table": "2.2.1"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "platform": {
      "php": "7.3.1"
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "@php -r \"copy('.env.example', '.env');\"",
      "@php -r \"unlink('composer.json');\"",
      "@php -r \"unlink('LICENSE.md');\"",
      "@php -r \"unlink('README.md');\"",
      "@php -r \"rename('composer.json.default', 'composer.json');\"",
      "@composer dump-autoload -o",
      "@php craft setup/welcome"
    ]
  }
}
